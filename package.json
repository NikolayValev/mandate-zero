    "test:e2e:ui": "playwright test --ui",
    "test:bot": "playwright test bot-play.spec.ts --headed",
    "check": "npm run typecheck && npm run lint && npm run test",
    "agent:loop": "node scripts/agent/continuous-improvement.mjs",
    "agent:loop:continuous": "node scripts/agent/continuous-improvement.mjs --continuous --interval-minutes 30"
  },
