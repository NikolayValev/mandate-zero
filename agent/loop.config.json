{
  "historyLimit": 40,
  "commandTimeoutMs": 300000,
  "commands": [
    {
      "id": "typecheck",
      "run": "npm run typecheck"
    },
    {
      "id": "lint",
      "run": "npm run lint"
    },
    {
      "id": "unit-tests",
      "run": "npm run test"
    },
    {
      "id": "e2e-mobile-tapflow",
      "run": "npx playwright test tests/e2e/mobile-tapflow.spec.ts",
      "envFlag": "AGENT_RUN_E2E",
      "defaultEnabled": false
    }
  ]
}
